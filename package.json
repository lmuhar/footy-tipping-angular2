    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "mocha": "^5.0.5",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "protractor": "^5.4.2",
    "rxjs-tslint": "^0.1.7",
    "ts-node": "~4.1.0",
    "tslint": "^5.20.1",
    "typescript": "~3.5.3"
  },
