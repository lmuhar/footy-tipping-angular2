    "morgan": "^1.9.0",
    "node-sass": "^4.13.1",
    "npm-check": "^5.6.0",
    "popper.js": "^1.16.0",
    "pretty-quick": "^1.11.1",
    "redis": "^2.8.0",
    "request": "^2.85.0",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
