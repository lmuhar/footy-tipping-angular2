        "@angular/platform-browser-dynamic": "^5.2.0",
        "@angular/router": "^5.2.0",
        "@sendgrid/mail": "^6.2.1",
        "angular-bootstrap-md": "^5.1.2",
        "angular2-jwt": "^0.2.3",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.2",
